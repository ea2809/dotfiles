{
  "rules": [
    {
      "description": "SpaceFn basic config", 
      "manipulators": [
        {
          "to_if_alone": {
            "key_code": "spacebar"
          }, 
          "from": {
            "modifiers": {
              "optional": [
                "any"
              ]
            }, 
            "key_code": "spacebar"
          }, 
          "to_after_key_up": {
            "set_variable": {
              "name": "spacefn_mode", 
              "value": 0
            }
          }, 
          "type": "basic", 
          "to_if_held_down": {
            "set_variable": {
              "name": "spacefn_mode", 
              "value": 1
            }
          }
        }
      ]
    }, 
    {
      "description": "SpaceFN: a to Alfred", 
      "manipulators": [
        {
          "conditions": [
            {
              "type": "variable_if", 
              "name": "spacefn_mode", 
              "value": 1
            }
          ], 
          "from": {
            "modifiers": {
              "optional": [
                "any"
              ]
            }, 
            "key_code": "a"
          }, 
          "type": "basic", 
          "to": {
            "shell_command": "open -a 'Alfred 4'"
          }
        }
      ]
    }, 
    {
      "description": "SpaceFN: b to spacebar", 
      "manipulators": [
